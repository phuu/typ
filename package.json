{
  "name": "typd",
  "description": "Runtime type checking for JavaScript",
  "scripts": {
    "build": "rm -rf ./dist && NODE_ENV=publish webpack --config config/webpack.config.publish.js --sort-assets-by --progress",
    "lint": "eslint config src",
    "prepublish": "npm run build",
    "specs": "NODE_ENV=test karma start config/karma.config.js",
    "specs:watch": "npm run specs -- --no-single-run",
    "test": "npm run specs && npm run lint",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "main": "dist/typd.js",
  "files": [
    "dist"
  ],
  "author": "Tom Ashworth <ta@twitter.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-plugin-typecheck": "^3.4.6",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-runtime": "^6.2.0",
    "eslint": "^2.6.0",
    "eslint-config-standard": "^4.3.1",
    "eslint-config-standard-react": "^1.0.4",
    "eslint-plugin-react": "^3.3.1",
    "eslint-plugin-standard": "^1.3.0",
    "immutable": "^3.7.5",
    "imports-loader": "^0.6.4",
    "karma": "^0.13.9",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "^0.1.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.2",
    "object-assign": "^4.0.1",
    "semantic-release": "^4.3.5",
    "unexpected": "^10.0.1",
    "webpack": "^1.12.1"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/phuu/typd.git"
  }
}
